package ch01.sec01;

import java.io.*;
import java.util.*;

public class Q17427 {

	public static void main(String[] args) {
		// 약수의 합 2
		// https://www.acmicpc.net/problem/17427
		
		int MAX = 1_000_000;
		long[] f = new long[MAX+1];
		long[] g = new long[MAX+1];
		
		// 1. f(n) 구하기
		for (int i = 1; i <= MAX; i++) {
			for (int j = 2; i * j <= MAX; j++) {
				f[i * j] += i;
			}
		}
	}

}
