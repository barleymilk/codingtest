package ch01.sec01;

import java.io.*;
import java.util.*;

public class Q17425 {

	public static void main(String[] args) throws Exception {
		// 약수의 합 
		// https://www.acmicpc.net/problem/17425
		
		// 최대 범위 설정 
		int MAX = 1000000;
		long[] f = new long[MAX + 1]; // f(n): n의 약수의 합
		long[] g = new long[MAX + 1]; // g(n): f(1) ~ f(n)의 총합 
		
		// 1. 배수의 원리로 f(n) 구하기 
		Arrays.fill(f,  1);
		
	}

}
